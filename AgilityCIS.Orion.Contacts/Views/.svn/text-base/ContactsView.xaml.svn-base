        <sdk:DataGrid  Grid.Row="0" MinHeight="250" Grid.Column="0" Style="{StaticResource DataGridStyle}"  Background="#34a266" ColumnHeaderStyle="{StaticResource DataGridHeaderGlassEffect}"  >
